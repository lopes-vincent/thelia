{
    "name": "thelia/thelia",
    "description": "Thelia is an ecommerce CMS.",
    "license": "LGPL-3.0+",
    "homepage": "http://thelia.net/",
    "keywords": [
        "ecommerce",
        "cms",
        "cms ecommerce",
        "ecommerce cms"
    ],
    "support": {
        "forum": "http://thelia.net/forum",
        "wiki": "http://doc.thelia.net"
    },
    "require": {
        "php": ">=7.2 <8.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-intl": "*",
        "ircmaxell/password-compat": "1.0.*",
        "psr/log": "1.1.*",
        "symfony/config": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dependency-injection": "5.2.*",
        "symfony/event-dispatcher": "5.2.*",
        "symfony/http-kernel": "5.2.*",
        "symfony/routing": "5.2.*",
        "symfony/filesystem": "5.2.*",
        "symfony/yaml": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony-cmf/routing": "2.3.*",
        "symfony/validator": "5.2.*",
        "symfony/options-resolver": "5.2.*",
        "symfony/security-core": "5.2.*",
        "symfony/security-http": "5.2.*",
        "symfony/security-csrf": "5.2.*",
        "symfony/security-guard": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/process": "5.2.*",
        "symfony/dom-crawler": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/finder": "5.2.*",
        "symfony/http-foundation": "5.2.*",
        "symfony/form": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "stack/builder": "1.0.*",
        "thelia/currency-converter": "~1.0",
        "doctrine/cache": "1.5.*",
        "ptachoire/cssembed": "1.0.*",
        "simplepie/simplepie": "1.3.*",
        "imagine/imagine": "0.7.1",
        "swiftmailer/swiftmailer": "5.4.*",
        "oyejorge/less.php": "1.7.*",
        "michelf/php-markdown": "1.6.*",
        "smarty/smarty": "3.1.33",
        "commerceguys/addressing": "1.1.*",
        "symfony/cache": "5.2.*",
        "spipu/html2pdf": "^5.0",
        "symfony/polyfill-php73": "^1.0",
        "symfony/lock": "5.2.*",
        "thelia/propel": "dev-thelia-2.5",
        "symfony/var-dumper": "^2.0|^3.0|^4.0",
        "assetic/framework": "^2.0",
        "symfony/mime": "5.2.*",
        "zircote/swagger-php": "3.1.*",
        "symfony/framework-bundle": "5.2.*",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "require-dev": {
        "fzaninotto/faker": "1.5.*",
        "thelia/hooktest-module": "~1.1",
        "thelia/hooktest-template": "~1.1",
        "phpunit/phpunit": "9.5.*",
        "symfony/browser-kit": "^5.2",
        "symfony/phpunit-bridge": "^5.2",
        "phpstan/phpstan": "^0.12.75",
        "phpstan/extension-installer": "^1.1"
    },
    "scripts": {
        "cs-diff": [
            "./bin/php-cs-fixer fix --dry-run --stop-on-violation --diff"
        ],
        "cs": [
            "./bin/php-cs-fixer fix"
        ],
        "phpstan": [
            "php -d memory_limit=-1 ./bin/phpstan analyse -c phpstan.neon"
        ],
        "test-unit": [
            "./bin/phpunit --configuration phpunit.xml.dist --testsuit unit"
        ],
        "test-functional": [
            "./bin/phpunit --configuration phpunit.xml.dist --testsuit functional"
        ],
        "test-legacy": [
            "./bin/phpunit --configuration phpunit.legacy.xml"
        ],
        "test": [
            "@test-unit",
            "@test-functional",
            "@test-legacy"
        ],
        "ci": [
            "@cs-diff",
            "@phpstan",
            "@test"
        ]
    },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Thelia\\": "core/lib/Thelia",
            "Thelia\\Model\\": "var/cache/propel/model/Thelia/Model",
            "TheliaMain\\": "var/cache/propel/database/TheliaMain"
        },
        "psr-0": {
            "": "local/modules/"
        },
        "files": [
            "core/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Thelia\\Tests\\Unit\\": "tests/Unit",
            "Thelia\\Tests\\Functional\\": "tests/Functional",
            "Thelia\\Tests\\": "tests/Legacy"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
